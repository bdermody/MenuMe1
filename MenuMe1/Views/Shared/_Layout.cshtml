<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MenuMe1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MenuMe1.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://use.typekit.net/iwm1iex.css">
</head>
<body style="background-color: #4d6e7f;">
    <header style="background-color: #ae4e4b; font-family: source-sans-pro, sans-serif; font-weight: 900" > 
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm  box-shadow mb-3" style="background-color: #ae4e4b;" >
            <div class="container-fluid">
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" style="color: #000000;"  asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" style="font: Times;" asp-area="" asp-controller="Restaurants" asp-action="Index">MisRestaurantes</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index">MiPerfil</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
       
    </header>
    <div class="container"style="background-color: #4d6e7f;">
        <main role="main" class="pb-3" >
            @RenderBody()
        </main>
    </div>

    <footer class="footer text-muted" style="background-color: #ae4e4b;">
        <div class="container">
            &copy; 2022 - MenuMe -
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
